import java.util.*;

public class trash {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
//A looping program, to print a required output.
        System.out.println("Enter a number: ");
        int num = sc.nextInt();

        // This int i will indicate the row we're on.
        for (int i = 0; i <= num; i++) {
            // This int j will indicate which column we're on.
            for (int j = 0; j <= num; j++) {
                //Used System.out.print instead of System.out.println for obvious reasons.
                System.out.print(i + j);
            }
            System.out.println();
        }
    }
}
